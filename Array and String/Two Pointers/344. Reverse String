class Solution {
    public void reverseString(char[] s) {
        // Applied Two Pointer Approach
        int left=0, right= s.length-1;
            while (left < right){
                // Performed Swap operation
                char temp=s[left];
                s[left]=s[right];
                s[right]=temp;
                left ++;
                right --;
            }
        }
    }
